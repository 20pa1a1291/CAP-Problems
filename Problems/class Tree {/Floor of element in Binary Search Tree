class Tree {
    int findFloor(Node root, int key) {
        if (root == null) return -1;
        if(root.data==key)
        return root.data;
        if(root.data>key)
        return findFloor(root.left,key);
        else {
           int floor=findFloor(root.right,key);
           return(floor<=key)?floor:root.data;
        }
        
        
    }
}
